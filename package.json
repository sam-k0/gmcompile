{
	"name": "gmcompile",
	"displayName": "Compile GML",
	"publisher": "samk0",
	"description": "",
	"icon": "icon.png",
	"version": "0.1.5",
	"repository": {"url": "https://github.com/sam-k0/gmcompile"},
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:gmcompile.compileGML"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "gmcompile.compileGML",
				"title": "compile GML"
			}
		],
		"keybindings":[
			{
				"command": "gmcompile.compileGML",
				"key": "ctrl+alt+f5"
			}
		],
		"configuration":{
			"title":"Compile GML",
			"properties":
			{
				"compilegml.compilerpath":
				{
					"type":"string",
					"default":"undefined",
					"description":"The external compiler location."
				}
			}
		}
		
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.67.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	}
}
